{
  "name": "zimara-ui",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14.x"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.3.1",
    "@patternfly/patternfly": "^4.108.2",
    "@patternfly/react-core": "^4.128.2",
    "d3": "^7.1.1",
    "konva": "^8.1.3",
    "react": "17.0.2",
    "react-konva": "^17.0.2-5",
    "react-router-dom": "^5.2.0",
    "react-router-last-location": "^2.0.1",
    "use-debounce": "^7.0.0",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@mdx-js/react": "^1.6.22",
    "@patternfly/react-icons": "^4.11.0",
    "@storybook/addon-a11y": "^6.4.0-alpha.22",
    "@storybook/addon-actions": "^6.4.0-beta.19",
    "@storybook/addon-docs": "^6.4.0-beta.10",
    "@storybook/addon-links": "^6.4.0-beta.10",
    "@storybook/builder-webpack5": "^6.3.12",
    "@storybook/manager-webpack5": "^6.3.12",
    "@storybook/node-logger": "^6.4.0-alpha.22",
    "@storybook/preset-create-react-app": "^4.0",
    "@storybook/react": "^6.4.0-beta.10",
    "@testing-library/dom": "^8.7.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.1.9",
    "@types/d3": "^7.0.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.6",
    "@types/react": "^17.0.29",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-dom": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@typescript-eslint/scope-manager": "^5.0.0",
    "babel-loader": "^8.2.2",
    "chromatic": "^6.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "25.0.5",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "monaco-editor": "^0.29.0",
    "node-sass": "^6.0.1",
    "prettier": "2.4.1",
    "prop-types": "^15.7.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "tslib": "^2.3.0",
    "typescript": "~4.3.5",
    "webpack": "5",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.8.0",
    "yaml-loader": "^0.6.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-storybook": "build-storybook -s public",
    "chromatic": "CHROMATIC_STORYBOOK_VERSION='@storybook/react@6.2.0' chromatic --exit-zero-on-changes",
    "eject": "react-scripts eject",
    "format": "prettier --write 'src/**/*.{js,ts,tsx,scss,css,json}'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx src --color",
    "storybook": "start-storybook -p 6006 -s public",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "react-app-env.d.ts",
      "src/components/Map.tsx",
      "src/index.tsx",
      "src/test-utils.tsx",
      "src/pages/App.tsx",
      "src/.*index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "lines": 90
      }
    }
  }
}
