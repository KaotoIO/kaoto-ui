{
  "name": "kaoto-ui",
  "version": "0.1.0",
  "private": true,
  "federatedModuleName": "kaoto",
  "engines": {
    "node": ">=14.x"
  },
  "scripts": {
    "start": "webpack serve --mode development --hot --progress --config webpack.dev.js ",
    "prod": "webpack serve --mode production --port 8080 --allowed-hosts all --progress --config webpack.prod.js ",
    "build": "webpack --mode production --config webpack.prod.js",
    "build-storybook": "build-storybook -s public",
    "chromatic": "npx chromatic",
    "clean": "rimraf dist",
    "e2e": "cypress open",
    "e2e:headless": "cypress run",
    "format": "prettier --check 'src'",
    "format:fix": "prettier --write --ignore-unknown 'src'",
    "lint": "eslint --ext .ts,.tsx,.js ./src",
    "lint:fix": "yarn lint --fix",
    "lint-staged": "yarn lint:fix && yarn format:fix",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 6006 -s public",
    "test": "jest src/**/*.test.ts*",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@patternfly/patternfly": "4.185.1",
    "@patternfly/react-code-editor": "^4.39.5",
    "@patternfly/react-core": "4.198.19",
    "@patternfly/react-icons": "4.49.19",
    "@rhoas/app-services-ui-shared": "^0.15.4",
    "ajv": "^8.8.2",
    "monaco-editor": "^0.32.1",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "3.1.4",
    "react-flow-renderer": "^9.7.3",
    "react-monaco-editor": "^0.47.0",
    "react-router-dom": "5.3.0",
    "react-router-last-location": "2.0.1",
    "react-shadow": "19.0.2",
    "uniforms": "^3.6.2",
    "uniforms-bridge-json-schema": "^3.6.2",
    "uniforms-patternfly": "^4.7.4",
    "use-debounce": "7.0.1",
    "uuid": "8.3.2",
    "web-vitals": "2.1.2",
    "yarn": "2.4.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.0-alpha.54",
    "@storybook/addon-docs": "^6.5.0-alpha.54",
    "@storybook/addon-links": "^6.5.0-alpha.54",
    "@storybook/builder-webpack5": "^6.5.0-alpha.54",
    "@storybook/manager-webpack5": "^6.5.0-alpha.54",
    "@storybook/node-logger": "^6.5.0-alpha.54",
    "@storybook/react": "^6.5.0-alpha.54",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.33",
    "@types/react-dom": "17.0.10",
    "@types/react-router-dom": "5.3.2",
    "@types/testing-library__jest-dom": "5.14.3",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "5.17.0",
    "chromatic": "^6.4.1",
    "css-loader": "^6.6.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cypress": "9.5.3",
    "dotenv-webpack": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest-dom": "3.9.2",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "4.12.4",
    "file-loader": "^6.2.0",
    "file-selector": "^0.4.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "lint-staged": "11.2.5",
    "mini-css-extract-plugin": "2.4.3",
    "null-loader": "^4.0.1",
    "prettier": "2.6.1",
    "prop-types": "15.7.2",
    "rimraf": "3.0.2",
    "source-map-loader": "3.0.0",
    "style-loader": "3.3.1",
    "ts-jest": "27.0.7",
    "ts-loader": "9.2.6",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "tslib": "^2.3.1",
    "typescript": "4.4.4",
    "url-loader": "4.1.1",
    "webpack": "5.71.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4",
    "webpack-merge": "^5.8.0",
    "whatwg-fetch": "^3.6.2",
    "yaml-loader": "0.6.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{tsx,ts}": "prettier --write",
    "*.tsx": "eslint --ext .tsx"
  },
  "packageManager": "yarn@3.1.0"
}
